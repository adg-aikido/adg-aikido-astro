---
import Layout from "../layouts/Layout.astro";
import HeroSection from "../components/sections/conversion/HeroSection.astro";
import BenefitsSection from "../components/sections/conversion/BenefitsSection.astro";
import HowItWorksSection from "../components/sections/conversion/HowItWorksSection.astro";
import CommunitySection from "../components/sections/conversion/CommunitySection.astro";
import PricingSection from "../components/sections/conversion/PricingSection.astro";
import NewsSection from "../components/sections/conversion/NewsSection.astro";
